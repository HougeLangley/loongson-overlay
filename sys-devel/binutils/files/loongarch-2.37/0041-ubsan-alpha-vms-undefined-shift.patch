+++ b/bfd/vms-alpha.c
@@ -7845,7 +7845,7 @@ evax_bfd_print_image (bfd *abfd, FILE *file)
 	}
       mask = bfd_getl32 (eihvn.subsystem_mask);
       for (j = 0; j < 32; j++)
